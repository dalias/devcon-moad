export const shirts = (styleIndex, colorIndex) => {
    let color = shirtColors[colorIndex]
    let style1 = shirt1(color)
    let style2 = shirt2(color)
    let style3 = shirt3(color)
    let style4 = shirt4(color)

    let array = [style1, style2, style3, style4]

    return {
        svg: array[styleIndex],
        color: color,
        colorArr: shirtColors,
        styleArr: array,
        curStyleIndex: styleIndex++ % array.length + array.length % array.length,
        curColorIndex: colorIndex++ % shirtColors.length + shirtColors.length % shirtColors.length
    }
}

export async function randomShirt() {
    let randColor = _.sample(shirtColors)
    let style1 = shirt1(randColor)
    let style2 = shirt2(randColor)
    let style3 = shirt3(randColor)
    let style4 = shirt4(randColor)

    let array = [style1, style2, style3, style4]
    let randomStyle = _.sample(array)
    const colorIndex = _.findIndex(shirtColors, function (color) { return color == randColor })
    const styleIndex = _.findIndex(array, function (svg) { return svg == randomStyle })

    return {
        svg: randomStyle,
        color: randColor,
        colorIndex: colorIndex,
        styleIndex: styleIndex
    }
}