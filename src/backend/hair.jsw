export async function hairs(styleIndex, colorIndex) {
    let color = await hairColors[colorIndex]

    let style1 = await hair1(color)
    let style2 = await hair2(color)
    let style3 = await hair3(color)
    let style4 = await hair4(color)
    let style5 = await hair5(color)
    let style6 = await hair6(color)
    let style7 = await hair7(color)
    let style8 = await hair8(color)
    let style9 = await hair9(color)
    let style10 = await hair10(color)
    let style11 = await hair11(color)
    let style12 = await hair12(color)
    let style13 = await hair13(color)
    let style14 = await hair14(color)
    let style15 = await hair15(color)
    let style16 = await hair16(color)
    let style17 = await hair17(color)
    let style18 = await hair18(color)
    let style19 = await hair19(color)
    let style20 = await hair20(color)
    let style21 = await hair21(color)
    let style22 = await hair22(color)
    let style23 = await hair23(color)

    let array = [style2, style3, style4, style5, style6, style7, style8, style9, style10, style11, style12, style14, style15, style16, style17, style18, style19, style20,style21, style22, style23, style1]

    return {
        svg: array[styleIndex],
        color: color,
        colorArr: hairColors,
        styleArr: array,
        curStyleIndex: styleIndex++ % array.length + array.length % array.length,
        curColorIndex: colorIndex++ % hairColors.length + hairColors.length % hairColors.length
    }
}

export async function randomHair() {
    let randomColor = _.sample(hairColors)
    let style1 = hair1(randomColor)
    let style2 = hair2(randomColor)
    let style3 = hair3(randomColor)
    let style4 = hair4(randomColor)
    let style5 = hair5(randomColor)
    let style6 = hair6(randomColor)
    let style7 = hair7(randomColor)
    let style8 = hair8(randomColor)
    let style9 = hair9(randomColor)
    let style10 = hair10(randomColor)
    let style11 = hair11(randomColor)
    let style12 = hair12(randomColor)
    let style13 = hair13(randomColor)
    let style14 = hair14(randomColor)
    let style15 = hair15(randomColor)
    let style16 = hair16(randomColor)
    let style17 = hair17(randomColor)
    let style18 = hair18(randomColor)
    let style19 = hair19(randomColor)
    let style20 = hair20(randomColor)
    let style21 = hair21(randomColor)
    let style22 = hair22(randomColor)
    let style23 = hair23(randomColor)

    let array = await [style2, style3, style4, style5, style6, style7, style8, style9, style10, style11, style12, style14, style15, style16, style17, style18, style19, style20,style21, style22, style23, style1]
    let randomStyle = await _.sample(array)
    let colorIndex = _.findIndex(hairColors, function (color) { return color == randomColor })
    let styleIndex = _.findIndex(array, function (svg) { return svg == randomStyle })
    return {
        svg: randomStyle,
        color: randomColor,
       colorIndex: colorIndex,
       styleIndex: styleIndex
    }
}