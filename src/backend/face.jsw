export async function faces(index) {
    const svg = await allFaces[index].svg
    const iLength = allFaces.length
    return { svg: svg, faceLength: iLength }
}

export async function randomFace() {
    let randomFace = _.sample(allFaces)
    const index = _.findIndex(allFaces, function (face) { return face.svg == randomFace.svg })

    return {svg: randomFace.svg , index:index}

}